                        </div>
                    </div>
                    <div class="row" style="margin-bottom:15px">
                        <div class="col-sm-6">
                            <div class="form-group">
                                <label class="col-sm-3">First Day of Business</label>
                                <div class="col-sm-9">
                                    @if (Model.FirstBusinessDay != null)
                                    {
                                        string.Format("{0:dd MMM yyyy}", Convert.ToDateTime(Html.DisplayFor(model => model.FirstBusinessDay).ToString()));
                                    }
                                </div>
                            </div>
                        </div>
                        <div class="col-sm-6">
                            <div class="form-group">
                                <label class="col-sm-3">Last Day of Business</label>
                                <div class="col-sm-9">
                                    @if (Model.LastBusinessDay != null)
                                    {
                                        string.Format("{0:dd MMM yyyy}", Convert.ToDateTime(Html.DisplayFor(model => model.LastBusinessDay).ToString()));
                                    }
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="row" style="margin-bottom:15px">
                        <div class="col-sm-12">
                            <div class="form-group">
                                <label class="col-sm-2">Remarks</label>
                                <div class="col-sm-12">
                                    @Html.TextAreaFor(model => model.TravelRemarks, htmlAttributes: new { @class = "form-control", @rows = "3", @placeholder = "Enter Travel Remarks..." })
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

