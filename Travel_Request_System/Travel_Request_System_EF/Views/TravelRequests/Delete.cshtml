                                <label class="col-sm-3">Drop Off Time</label>
                                <div class="col-sm-9">
                                    @if (Model.DropOffTime != null)
                                    {
                                        string.Format("{0:hh:mm tt}", Convert.ToDateTime(Html.DisplayFor(model => model.DropOffTime).ToString()));
                                    }
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="row" style="margin-bottom:15px">
                        <div class="col-sm-6">
                            <div class="form-group">
                                <label class="col-sm-3">Preferred Vehicle Type</label>
                                <div class="col-sm-9">
                                    @Html.DisplayFor(model => model.PreferredVehicle, new { htmlAttributes = new { @class = "form-control" } })
                                </div>
                            </div>
                        </div>
                        <div class="col-sm-6">
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <div class="col-xs-12 col-md-12">
        <div class="box box-default">
            <div class="box-header with-border">
                <h3 class="box-title">Remarks</h3>
                <div class="box-tools pull-right">
                    <button type="button" class="btn btn-box-tool" data-widget="collapse"><i class="fa fa-minus"></i></button>
                </div>
            </div>
            <div class="box-body">
                <div class="row" style="margin-bottom:15px">
                    <div class="col-sm-12">
                        <div class="form-group">
                            <div class="col-sm-12">
                                @Html.TextAreaFor(model => model.Remarks, htmlAttributes: new { @class = "form-control", @rows = "4", @placeholder = "Enter Travel Request Remarks..." })
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

    @using (Html.BeginForm())
    {
        @Html.AntiForgeryToken()

        <div class="row" style="margin-bottom:15px">
            <div class="col-md-6">
                <div class="form-group">
                    <div class="col-md-3">
                        <input type="submit" class="form-control btn btn-info btn-primary" value="Delete" />
                    </div>
                    <div class="col-md-3">
                        @Html.ActionLink("Back to List", "Index", null, new { @class = "form-control btn btn-info" })
                    </div>
                </div>
            </div>
        </div>
    }
</div>
@section Scripts {
    <script type="text/javascript">
        if ($('#airportpic').text() == "Yes") {
            $('#PickUpDetailsDiv').show()
        }
        else {
            $('#PickUpDetailsDiv').hide()
        }

        $('#detailsDiv').find('textarea').attr('disabled', 'disabled');

    </script>
}
