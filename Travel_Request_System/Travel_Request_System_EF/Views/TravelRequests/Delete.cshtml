                </div>
            </div>
            <div class="box-body">
                <div>
                    <div style="margin-bottom:15px">
                    </div>
                    <div class="row" style="margin-bottom:15px">
                        <div class="col-sm-6">
                            <div class="form-group">
                                <label class="col-sm-3">Departure Date</label>
                                <div class="col-sm-9">
                                    @if (Model.DepartureDate != null)
                                    {
                                        string.Format("{0:dd MMM yyyy}", Convert.ToDateTime(Html.DisplayFor(model => model.DepartureDate).ToString()));
                                    }
                                </div>
                            </div>
                        </div>
                        <div class="col-sm-6">
                            <div class="form-group">
